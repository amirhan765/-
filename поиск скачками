#include <iostream>
#include <cmath>   // Для функции sqrt
using namespace std;

int jumpSearch(int arr[], int n, int x) {
    // Вычисляем размер шага
    int step = sqrt(n);
    int prev = 0;

    // Находим блок, в котором может находиться элемент
    while (arr[min(step, n)-1] < x) {
        prev = step;
        step += sqrt(n);
        if (prev >= n)
            return -1; // Не нашли, элемент отсутствует
    }

    // Линейный поиск внутри блока
    for (int i = prev; i < min(step, n); i++) {
        if (arr[i] == x)
            return i; // Возвращаем индекс найденного элемента
    }

    return -1; // Не нашли
}

int main() {
    int arr[] = {1, 3, 5, 7, 9, 11, 13, 15};
    int n = sizeof(arr) / sizeof(arr[0]);
    int x = 13;

    int index = jumpSearch(arr, n, x);

    if (index != -1)
        cout << "Элемент " << x << " найден по индексу " << index << endl;
    else
        cout << "Элемент " << x << " не найден" << endl;

    return 0;
}
