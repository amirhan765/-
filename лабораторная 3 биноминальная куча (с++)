int getMax() {
  int res = 0;
  int resPos = -1;
  for (int i=0;i<SIZE;i++) {
    if (mas[i] && mas[i]->data > res) {
      res = mas[i]->data;
      resPos = i;
    }
  }
  Node** tmp = new Node*[SIZE];
  memset(tmp,0,sizeof(tmp)*SIZE);
  Node* cur = mas[resPos]->left;
  for (int i=resPos-1;i>=0;i--) {
    tmp[i] = new Node(*cur);
    cur = cur->right;
    tmp[i]->right = 0;
  }
  delete mas[resPos];
  mas[resPos] = 0;
 
  joinBQ(mas, tmp);
  delete tmp;
  size--;
  return res;
}
